{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "components_arm_worshop_name": {
            "defaultValue": "arm-worshop",
            "type": "String"
        },
        "serverfarms_bekk_name": {
            "defaultValue": "bekk",
            "type": "String"
        },
        "sites_arm_worshop_name": {
            "defaultValue": "arm-worshop",
            "type": "String"
        },
        "repoURL": {
            "type": "string",
            "defaultValue": "https://github.com/HenrikWM/arm-workshop.git"
        },
        "branch": {
            "type": "string",
            "defaultValue": "master"
        }
    },
    "variables": {},
    "resources": [
        {
            "comments": "Generalized from resource: '/subscriptions/233f5e0e-5e02-47c3-b6fa-84737a6089a8/resourceGroups/bekk/providers/microsoft.insights/components/arm-worshop'.",
            "type": "microsoft.insights/components",
            "kind": "web",
            "name": "[parameters('components_arm_worshop_name')]",
            "apiVersion": "2014-04-01",
            "location": "Central US",
            "tags": {
                "hidden-link:/subscriptions/233f5e0e-5e02-47c3-b6fa-84737a6089a8/resourceGroups/bekk/providers/Microsoft.Web/sites/arm-worshop": "Resource"
            },
            "properties": {
                "ApplicationId": "[parameters('components_arm_worshop_name')]"
            },
            "dependsOn": []
        },
        {
            "comments": "Generalized from resource: '/subscriptions/233f5e0e-5e02-47c3-b6fa-84737a6089a8/resourceGroups/bekk/providers/Microsoft.Web/serverfarms/bekk'.",
            "type": "Microsoft.Web/serverfarms",
            "sku": {
                "name": "B1",
                "tier": "Basic",
                "size": "B1",
                "family": "B",
                "capacity": 1
            },
            "name": "[parameters('serverfarms_bekk_name')]",
            "apiVersion": "2015-08-01",
            "location": "North Europe",
            "properties": {
                "name": "[parameters('serverfarms_bekk_name')]",
                "numberOfWorkers": 1
            },
            "dependsOn": []
        },
        {
            "comments": "Generalized from resource: '/subscriptions/233f5e0e-5e02-47c3-b6fa-84737a6089a8/resourceGroups/bekk/providers/Microsoft.Web/sites/arm-worshop'.",
            "type": "Microsoft.Web/sites",
            "name": "[parameters('sites_arm_worshop_name')]",
            "apiVersion": "2015-08-01",
            "location": "North Europe",
            "tags": {
                "hidden-related:/subscriptions/233f5e0e-5e02-47c3-b6fa-84737a6089a8/resourcegroups/bekk/providers/Microsoft.Web/serverfarms/bekk": "empty"
            },
            "properties": {
                "name": "[parameters('sites_arm_worshop_name')]",
                "hostNames": [
                    "[concat(parameters('sites_arm_worshop_name'),'.azurewebsites.net')]"
                ],
                "serverFarmId": "[resourceId('Microsoft.Web/serverfarms', parameters('serverfarms_bekk_name'))]"
            },
            "resources": [
        {
          "apiVersion": "2015-08-01",
          "name": "web",
          "type": "sourcecontrols",
          "dependsOn": [
            "[resourceId('Microsoft.Web/Sites', parameters('sites_arm_worshop_name'))]"
          ],
          "properties": {
            "RepoUrl": "[parameters('repoURL')]",
            "branch": "[parameters('branch')]",
            "IsManualIntegration": true
          }
        }],
            "dependsOn": [
                "[resourceId('Microsoft.Web/serverfarms', parameters('serverfarms_bekk_name'))]"
            ]
        }
    ]
}
